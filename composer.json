{
    "name": "wieni/wmrecaptcha",
    "type": "drupal-module",
    "description": "Provides an integration with Google's reCAPTCHA service in Drupal 8",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=8.1",
        "drupal/core": "^9 || ^10.1",
        "guzzlehttp/guzzle": "^7"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\wmsubscription\\": "src/"
        }
    },
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php",
            "phpstan analyse"
        ]
    }
}
